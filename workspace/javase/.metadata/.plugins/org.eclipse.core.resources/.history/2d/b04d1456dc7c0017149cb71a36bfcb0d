package com.itheima;
/*抽象
 * */
public class ExtendsDemo5 {
	public static void main(String[] args) {
		
	}
}
abstract class Animal {
	public abstract  void eat();
	public void run(){
		
	}
}
class Dog extends Animal{
	public void eat(){
		System.out.println("我吃骨头");
	}
}
abstract class Cat extends Animal{
	
}
