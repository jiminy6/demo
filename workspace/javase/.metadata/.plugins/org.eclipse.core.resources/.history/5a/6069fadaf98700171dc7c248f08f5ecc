package com.itheima_8;

public class Money {
	private int value;
	private boolean flag;
	public Money(boolean b, int i) {
		
	}
	public int getValue() {
		return value;
	}
	public void setValue(int value) {
		this.value = value;
	}
	public boolean isFlag() {
		return flag;
	}
	public void setFlag(boolean flag) {
		this.flag = flag;
	}
	public Money() {
		
	}

}
