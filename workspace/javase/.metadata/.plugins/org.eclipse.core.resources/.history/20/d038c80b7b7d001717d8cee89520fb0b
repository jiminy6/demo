package system;

public class ZhengZeDemo {
	public static void main(String[] args) {
		String qq="12345";
//		boolean flag=qq.matches("[1-9][0-9][4-14]");
//		System.out.println(flag);
		Check(qq);
	}
	public static boolean Check(String s){
		s.toLowerCase();
		char ch=s.charAt(2);
		String str="aeiou";
		return str.contains(ch+"");
	}
}
